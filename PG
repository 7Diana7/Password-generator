import random

print("soy un programa que genera contraseña")
Quest = int(input("de cuantos caracteres quieres tu contraseña?: "))
Password = "";
elements = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

for i in range(Quest):
    Password+= random.choice(elements)

print(Password)
